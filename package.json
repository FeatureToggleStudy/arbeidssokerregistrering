{
  "name": "arbeidssokerregistrering",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts-ts": "2.8.0"
  },
  "scripts": {
    "start:ts": "react-scripts-ts start",
    "build:ts": "react-scripts-ts build",
    "deploy": "npm run build && gh-pages -d build",
    "test": "react-scripts-ts test --env=jsdom",
    "lint": "tslint ./src/**/*.ts*",
    "lint:fix": "tslint ./src/**/*.ts* --fix",
    "test:coverage": "npm test -- --coverage",
    "prettier": "prettier --write --single-quote --tab-width 4 src/**/*.ts src/**/*.tsx src/**/*.less",
    "eject": "react-scripts-ts eject",
    "start:less": "autoless --source-map --intervall 100 ./src ./public",
    "build:less": "node ./node_modules/less/bin/lessc/ src/index.less ./public/index.css",
    "start": "npm-run-all -p start:*",
    "build:tekster": "node ./src/utils/build-tekster.js",
    "build": "npm run build:less && npm run build:ts && npm run build:tekster"
  },
  "devDependencies": {
    "@types/jest": "21.1.8",
    "@types/node": "8.0.53",
    "@types/react": "16.0.25",
    "@types/react-dom": "16.0.3",
    "autoless": "0.1.7",
    "less": "2.7.3",
    "npm-run-all": "4.0.2"
  }
}
